      "post": {
        "tags": [
          "teams"
        ],
        "summary": "Add a new team",
        "description": "Pass a team object without id field to be added to the database. Returns team object added to the database with the teamId field.",
        "operationId": "addTeam",
        "requestBody": {
          "description": "Team object to be added to the database",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TeamPost"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "integer",
                      "format": "int64",
                      "example": 200
                    },
                    "status": {
                      "type": "string",
                      "example": "OK"
                    },
                    "message": {
                      "type": "string",
                      "example": "Added team"
                    },
                    "response": {
                      "$ref": "#/components/schemas/Team"
                    },
                    "links": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Link"
                      }
                    }
                  }
                }
              }
            }
          },
          "405": {
            "description": "Invalid input",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-codegen-request-body-name": "body"
      }
