          "headcoach",
          "international_titles",
          "league",
          "name",
          "org_location"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Cloud9"
          },
          "acronym": {
            "type": "string",
            "example": "C9"
          },
          "headcoach": {
            "type": "string",
            "example": "Kim \"Reignover\" Yeu-jin"
          },
          "created": {
            "type": "string",
            "example": "2012-12-03T00:00:00.000Z"
          },
          "domestic_titles": {
            "type": "integer",
            "format": "int64",
            "example": 1
          },
          "international_titles": {
            "type": "integer",
            "format": "int64",
            "example": 0
          },
          "org_location": {
            "type": "string",
            "example": "United States"
          },
          "league": {
            "type": "string",
            "example": "LCS"
          }
        }
      },
      "Player": {
        "required": [
          "contract_ends",
          "country",
          "name",
          "player",
          "playerid",
          "role",
          "surname"
        ],
        "type": "object",
        "properties": {
          "playerid": {
            "type": "integer",
            "format": "int64"
          },
          "player": {
            "type": "string",
            "example": "Perkz"
          },
          "name": {
            "type": "string",
            "example": "Luka"
          },
          "surname": {
            "type": "string",
            "example": "Perković"
          },
          "country": {
            "type": "string",
            "example": "Croatia"
          },
          "team": {
            "type": "string",
            "example": "Cloud9"
          },
          "role": {
            "type": "string",
            "example": "Mid Laner"
          },
          "contract_ends": {
            "type": "string",
            "example": "2023-11-20T00:00:00.000Z"
          }
        }
      },
      "Error": {
        "required": [
          "code",
          "message",
          "status"
        ],
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "string"
          },
          "message": {
            "type": "string"
