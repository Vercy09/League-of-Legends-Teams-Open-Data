  "components": {
    "schemas": {
      "Team": {
        "required": [
          "acronym",
          "created",
          "domestic_titles",
          "headcoach",
          "international_titles",
          "league",
          "name",
          "org_location",
          "teamid"
        ],
        "type": "object",
        "properties": {
          "teamid": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string",
            "example": "Cloud9"
          },
          "acronym": {
            "type": "string",
            "example": "C9"
          },
          "headcoach": {
            "type": "string",
            "example": "Kim \"Reignover\" Yeu-jin"
          },
          "created": {
            "type": "string",
            "example": "2012-12-03T00:00:00.000Z"
          },
          "domestic_titles": {
            "type": "integer",
            "format": "int64",
            "example": 1
          },
          "international_titles": {
            "type": "integer",
            "format": "int64",
            "example": 0
          },
          "org_location": {
            "type": "string",
            "example": "United States"
          },
          "league": {
            "type": "string",
            "example": "LCS"
          }
        }
      },
      "TeamPost": {
        "required": [
          "acronym",
          "created",
